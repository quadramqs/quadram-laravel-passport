{
    "name": "quadram/laravel-passport",
    "description": "This package install Laravel Passport library and gives you some useful resources to issue expiring and not expiring tokens for your users",
    "type": "library",
    "require-dev": {
        "orchestra/testbench": "^5.0@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "VÃ­ctor",
            "email": "vlopez@quadram.mobi"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/passport": "^9.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Quadram\\LaravelPassport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Quadram\\LaravelPassport\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Quadram\\LaravelPassport\\Providers\\LaravelPassportServiceProvider"
            ],
            "aliases": {
                "LaravelPassport": "Quadram\\LaravelPassport\\Facade"
            }
        }
    }
}
